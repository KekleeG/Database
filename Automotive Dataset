                          | car club   |--------| bookings |
                          |------------|        |----------|



        --  ENTITY RELATIONAL MODEL  --


          CarModel_T               Vendor_T                            CarMake_T
     |PK| ModelID               | PK |VendorID                      |PK | MakeID
          model color                 vendor name                         make year
          model type                  vendor email                        make name
          model year                  vendor address                      top speed
          body style                  vendor work phone                   gear type
          engine type                 vendor postal code                  fuel grade
          model name                  LocationID                          modelID
          makeID


           Sponsor_T                     Location_T                         Promoter_T
         |PK| MakeID                  |PK | LocationID                   |PK| PromoterID       
           sponsor name                     location name                     promoter name
           sponsor email                    location address                  promoter email
           sponsor address                  location zip code                 promoter address
           sponsor work                     location lot space                promoter zipcode
           phone                            location phonenumber              promoter gender
           sponsor zipcode                  PromoterID                        SponsorID
           PromoterID


               CarClub_T                                  Booking_T
         |PK|   clubID                                |PK| LocationID
                ModelID                                    PromoterID
                car club size                              ClubID
                car club city
                car club social media
                car club state
                car club email
                car club name ModelID








--SQL DATABASE CODE --



CREATE TABLE CarModel_T
(Model_ID NUMERIC(5,0) NOT NULL UNIQUE,
ModelName VARCHAR(15) NOT NULL,
ModelColor VARCHAR(15) NOT NULL,
ModelType VARCHAR(20) NOT NULL,
ModelYear INT(4) NOT NULL,
EngineType VARCHAR(15) NOT NULL,
Make_ID NUMERIC(5,0) NOT NULL,
CONSTRAINT PK_CarModel PRIMARY KEY(Model_ID));


CREATE TABLE CarMake_T
(Make_ID NUMERIC(5,0) NOT NULL UNIQUE,
Model_ID NUMERIC(5,0) NOT NULL,
Make_Name VARCHAR(20) NOT NULL,
Make_Year INT(4) NOT NULL,
Top_Speed INT(3) NOT NULL,
Gear_Type VARCHAR(15) NOT NULL,
Fuel_Grade VARCHAR(25) NOT NULL,
CONSTRAINT PK_CARMAKE PRIMARY KEY(Make_ID));


CREATE TABLE CarClub_T
(Club_ID NUMERIC(5,0) NOT NULL UNIQUE,
Club_Name VARCHAR(75) NOT NULL,
Club_Email VARCHAR(50) NOT NULL,
Club_Size INT(3) NOT NULL,
Club_City VARCHAR(50),
Club_State VARCHAR(15),
Club_Social VARCHAR(60) NOT NULL,
Model_ID NUMERIC(5,0) NOT NULL,
CONSTRAINT PK_CarClub PRIMARY KEY(Club_ID));


CREATE TABLE Promoter_T
(PROMOTER_ID NUMERIC(5,0) NOT NULL UNIQUE,
P_NAME VARCHAR(70) NOT NULL,
P_EMAIL VARCHAR(35) NOT NULL,
P_ADDR VARCHAR(75),
P_CITY VARCHAR(50),
P_STATE VARCHAR(50),
P_ZIP_CODE INT(5),
P_GENDER VARCHAR(6) NOT NULL,
Vendor_ID NUMERIC(5,0) NOT NULL,
CONSTRAINT PK_PROMOTER PRIMARY KEY(PROMOTER_ID));


CREATE TABLE Location_T
(LOCATION_ID NUMERIC(5,0) NOT NULL UNIQUE,
PROMOTER_ID NUMERIC(5,0) NOT NULL,
L_NAME VARCHAR(30) NOT NULL,
L_PHONENUMBER NUMERIC(10,0) NOT NULL,
L_ADDR VARCHAR(75),
L_CITY VARCHAR(50),
L_STATE VARCHAR(50),
L_ZIP_CODE INT(5),
L_LOT_SPACE INT(255) NOT NULL,
CONSTRAINT PK_LOCATION PRIMARY KEY(LOCATION_ID));


CREATE TABLE Vendor_T
(VENDOR_ID NUMERIC(5,0) NOT NULL UNIQUE,
V_NAME VARCHAR(30) NOT NULL,
LOCATION_ID NUMERIC(5,0) NOT NULL,
V_EMAIL VARCHAR(50) NOT NULL,
V_PHONE NUMERIC(10,0) NOT NULL,
V_ADDR VARCHAR(100),
V_CITY VARCHAR(50),
V_STATE VARCHAR(50),
V_ZIP_CODE INT(5),
CONSTRAINT PK_VENDOR PRIMARY KEY(VENDOR_ID));


CREATE TABLE Sponsor_T
(SPONSOR_ID NUMERIC(5,0) NOT NULL UNIQUE,
S_NAME VARCHAR(50) NOT NULL,
S_EMAIL VARCHAR(35) NOT NULL,
S_PHONE NUMERIC(10,0) NOT NULL,
S_ADDR VARCHAR(75),
S_CITY VARCHAR(50),
S_STATE VARCHAR(50),
S_ZIPCODE INT(5),
PROMOTER_ID NUMERIC(5,0) NOT NULL,
CONSTRAINT PK_SPONSOR PRIMARY KEY(SPONSOR_ID));





