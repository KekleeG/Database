/* Add Foreign Keys */
ALTER TABLE CarModel_T ADD
CONSTRAINT FK_CARMODEL FOREIGN KEY(Make_ID) REFERENCES CarMake_T(Make_ID);

ALTER TABLE CarMake_T ADD
CONSTRAINT FK_CARMAKE  FOREIGN KEY(Model_ID) REFERENCES CarModel_T(Model_ID);

ALTER TABLE CarClub_T ADD
CONSTRAINT FK_CARCLUB FOREIGN KEY(Model_ID) REFERENCES CarModel_T(Model_ID);

ALTER TABLE Promoter_T ADD
CONSTRAINT FK_PROMOTER FOREIGN KEY(VENDOR_ID) REFERENCES Vendor_T(Vendor_ID);

ALTER TABLE Location_T ADD
CONSTRAINT FK_LOCATION FOREIGN KEY(PROMOTER_ID) REFERENCES Promoter_T(PROMOTER_ID);

ALTER TABLE Vendor_T ADD
CONSTRAINT FK_VENDOR FOREIGN KEY(LOCATION_ID) REFERENCES Location_T(LOCATION_ID);

ALTER TABLE Sponsor_T ADD
CONSTRAINT FK_SPONSER FOREIGN KEY(PROMOTER_ID) REFERENCES Promoter_T(PROMOTER_ID);








--RETRIEVE DATA FROM DATABASE-- 

--1-Find Model_Types with more than 3 models:
SELECT ModelType, count(*) FROM CarModel_T GROUP BY ModelType;


--2-Query: list club name, size  greater then 100?
SELECT Club_Name, Club_Size FROM CarClub_T WHERE Club_Size < 100;


--3-for cars between 90 - 150 mph, what are the make names and top speed:
SELECT Make_Name , Top_Speed FROM CarMake_T WHERE Top_Speed BETWEEN 90 AND 150;


--4- find all location names from the states inputed on location table
 SELECT L_Name, L_State FROM Location_T;


--5-what are all of the sponsors  from the state Virginia:
 SELECT * FROM  Sponsor_T WHERE S_State = 'Virginia';


 ---6 -what are the make, model id, make name, year speed,gear type and fuel grade for all FORD vehicles:
 SELECT * FROM CarMake_T WHERE Make_Name = 'FORD';


 --7- what are the make, model id, make name, year speed,gear type and fuel grade for all vehicles  with speeds > then 100mph
 SELECT * FROM CarMake_T WHERE Top_Speed >100;

--8-what are model types and model names from car model table:
 SELECT CarModel_T.ModelType,CarModel_T.ModelName FROM CarModel_T;


 --9- what are the make, model id, make name, year speed,gear type and fuel grade for all vehicles that were made in 2018 only:
SELECT * FROM CARMAKE_T WHERE CARMAKE_T.Make_Year = 2018;

 --10-- how many locations are their from this table;
SELECT COUNT(*) FROM Location_T;

--11--for every model, what is the count that we have;
 SELECT ModelType, count(*) FROM CarModel_T GROUP BY ModelType;


--12-in comparision,from car club table what are the id numers, club name, email, city,state and social along
--   with the model year, engine type, name, color and id from the car model table?
SELECT * FROM CarClub_T JOIN CarModel_T ON CarClub_T.Model_ID = CarModel_T.Model_ID;


--13-- what are all make names from car make table with model names from car model table;
SELECT CarMake_T.Make_Name, CarModel_T.ModelName FROM CarMake_T JOIN CarModel_T ON
CarMake_T.Model_ID = CarModel_T.Model_ID;

--14--find sponsor names and promoter names for both tables ;
SELECT Sponsor_T.S_Name,Promoter_T.P_NAME FROM Promoter_T LEFT JOIN Sponsor_T ON Sponsor_T.Promoter_ID = Promoter_T.Promoter_ID;


--14-- select * from CarModel_T where ModelColor = 'Green';
select * from CarModel_T where ModelColor = 'Green';







